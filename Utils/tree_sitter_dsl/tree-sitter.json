{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "dsl",
      "camelcase": "Dsl",
      "title": "Dsl",
      "scope": "source.dsl",
      "file-types": [
        "dsl"
      ],
      "injection-regex": "^dsl$",
      "class-name": "TreeSitterDsl"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "Dsl grammar for tree-sitter",
    "authors": [
      {
        "name": "hzc"
      }
    ],
    "links": {
      "repository": "https://github.com/tree-sitter/tree-sitter-dsl"
    }
  },
  "bindings": {
    "c": true,
    "go": false,
    "node": true,
    "python": true,
    "rust": true,
    "swift": false,
    "zig": false
  }
}